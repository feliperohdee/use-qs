{
	"dependencies": {
		"lodash": "^4.17.21"
	},
	"devDependencies": {
		"@types/lodash": "^4.17.13",
		"@vitest/coverage-v8": "2.1.8",
		"prettier": "^3.4.2",
		"typescript": "^5.7.2",
		"vitest": "^2.1.8"
	},
	"files": [
		"dist"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"name": "use-qs",
	"repository": {
		"type": "git",
